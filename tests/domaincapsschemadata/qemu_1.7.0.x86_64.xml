      <model>Conroe</model>
      <model>n270</model>
      <model>athlon</model>
      <model>pentium3</model>
      <model>pentium2</model>
      <model>pentium</model>
      <model>486</model>
      <model>coreduo</model>
      <model>kvm32</model>
      <model>qemu32</model>
      <model>kvm64</model>
      <model>core2duo</model>
      <model>phenom</model>
      <model>qemu64</model>
