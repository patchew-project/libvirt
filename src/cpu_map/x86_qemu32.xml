<!-- extra info from qemu:
  "model.level": "4"
  "model.stepping": "3"
  "model.xlevel": "0x80000004"
  "model.model_id": "QEMU Virtual CPU version  QEMU_HW_VERSION"
-->
<cpus>
  <model name="qemu32">
    <decode host="on" guest="on"/>
    <signature family="6" model="6"/>
    <vendor name="Intel"/>
    <feature name="apic"/>
    <feature name="cmov"/>
    <feature name="cx8"/>
    <feature name="de"/>
    <feature name="fpu"/>
    <feature name="fxsr"/>
    <feature name="mce"/>
    <feature name="mmx"/>
    <feature name="msr"/>
    <feature name="pae"/>
    <feature name="pat"/>
    <feature name="pge"/>
    <feature name="pni"/>
    <feature name="pse"/>
    <feature name="sep"/>
    <feature name="sse"/>
    <feature name="sse2"/>
    <feature name="tsc"/>
  </model>
</cpus>
