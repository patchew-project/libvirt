#
# The udevhelper binary reports:
#   UID GID SELINUX
#
ACTION!="add|change", GOTO="libvirt_end"
SUBSYSTEM!="block", GOTO="libvirt_end"
PROGRAM="/usr/libexec/libvirt_udevhelper", OWNER="%c{1}",GROUP="%c{2}", SECLABEL{selinux}="%c{3}"
LABEL="libvirt_end"
